{
	"name": "testgd",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite --port=12200",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"lint": "eslint src",
		"lint:fix": "eslint src --fix",
		"lint:watch": "nodemon --watch src --ext ts --exec \"npm run lint\"",
		"ts:watch": "tsc --watch --noEmit",
		"render": "npm run render:run && npm run render:spritesheets && npm run render:prepareExport && npm run render:export && npm run render:clean",
		"render:spritesheets": "node blender/scripts/packageSpritesheets/packageSpritesheets.js",
		"render:run": "blender -b blender/testgd.blend -P blender/scripts/render/main.py",
		"render:prepareExport": "rm -rf src/assets/spritesheets/* src/assets/spritesheetsDatas.json",
		"render:export": "cp -r blender/tmp/ src/assets",
		"render:clean": "rm -rf blender/tmp"
	},
	"devDependencies": {
		"@eslint/js": "^9.12.0",
		"@types/eslint__js": "^8.42.3",
		"@types/lodash": "^4.17.9",
		"@types/node": "^22.7.2",
		"@typescript-eslint/eslint-plugin": "^8.7.0",
		"@typescript-eslint/parser": "^8.7.0",
		"eslint": "^9.12.0",
		"image-size": "^1.1.1",
		"nodemon": "^3.1.7",
		"ts-node": "^10.9.2",
		"tslib": "^2.7.0",
		"typescript": "^5.6.2",
		"typescript-eslint": "^8.7.0",
		"vite": "^5.4.8"
	},
	"dependencies": {
		"lodash": "^4.17.21",
		"pixi.js": "^8.4.1"
	}
}
