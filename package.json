{
	"name": "testgd",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite --port=12200",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"render": "npm run render:run && npm run render:prepareExport && npm run render:export && npm run render:clean",
		"render:run": "blender -b blender/testgd.blend -P blender/scripts/render/main.py",
		"render:prepareExport": "rm -rf src/assets/images && mkdir -p src/assets/images",
		"render:export": "cp -r blender/tmp/* src/assets/images/",
		"render:clean": "rm -rf blender/tmp"
	},
	"devDependencies": {
		"@types/lodash": "^4.14.202",
		"@typescript-eslint/eslint-plugin": "^6.18.1",
		"@typescript-eslint/parser": "^6.18.1",
		"eslint": "^8.56.0",
		"eslint-plugin-vue": "^9.19.2",
		"pixi.js": "^7.3.3",
		"typescript": "^5.2.2",
		"vite": "^5.0.8",
		"vite-plugin-eslint": "^1.8.1"
	},
	"dependencies": {
		"lodash": "^4.17.21"
	}
}
