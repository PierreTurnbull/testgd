{
	"name": "testgd",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite --port=12200",
		"dev:editor": "node src/app/editor/server/server.js",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"lint": "eslint src",
		"lint:fix": "eslint src --fix",
		"lint:watch": "nodemon --watch src --ext ts --exec \"npm run lint\"",
		"ts:watch": "tsc --watch --noEmit",
		"render": "npm run render:run && npm run render:package && npm run render:export && npm run render:clean",
		"render:package": "node blender/scripts/packageAssets/packageAssets.js",
		"render:run": "blender -b blender/testgd.blend -P blender/scripts/render/main.py",
		"render:export": "node blender/scripts/exportAssets/exportAssets.js",
		"render:clean": "rm -rf blender/tmp",
		"test": "vitest",
		"test:cov": "vitest --coverage",
		"workers:build": "sh scripts/buildWorkers/buildWorkers.sh"
	},
	"devDependencies": {
		"@eslint/js": "^9.12.0",
		"@stylistic/eslint-plugin-ts": "^2.9.0",
		"@types/cors": "^2.8.17",
		"@types/eslint__js": "^8.42.3",
		"@types/express": "^5.0.0",
		"@types/lodash": "^4.17.9",
		"@types/node": "^22.7.2",
		"@typescript-eslint/eslint-plugin": "^8.19.0",
		"@typescript-eslint/parser": "^8.7.0",
		"@vitest/coverage-v8": "^2.1.4",
		"esbuild": "^0.24.2",
		"eslint": "^9.12.0",
		"express": "^4.21.2",
		"globals": "^15.14.0",
		"image-size": "^1.1.1",
		"jsdom": "^25.0.1",
		"nodemon": "^3.1.7",
		"tslib": "^2.7.0",
		"typescript": "^5.7.2",
		"typescript-eslint": "^8.19.0",
		"vite": "^5.4.8",
		"vitest": "^2.1.4"
	},
	"dependencies": {
		"body-parser": "^1.20.3",
		"cors": "^2.8.5",
		"detect-collisions": "^9.17.0",
		"lodash": "^4.17.21",
		"pixi.js": "^8.4.1",
		"preact": "^10.26.2"
	}
}
